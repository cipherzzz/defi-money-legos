  "description": "example code using the money legos",
  "main": "truffle-config.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts' --timeout 10000",
